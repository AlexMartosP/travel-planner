{
  "name": "travel-planner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "supabase start && next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "setup": "ts-node ./scripts/createStorageBucket.ts",
    "db:start": "supabase start",
    "db:generate-seed": "npx tsx seed.mts > supabase/seed.sql",
    "db:setup-snaplet": "npx snaplet setup",
    "db:generate-types": "sh ./scripts/generateDbTypes.sh",
    "db:stop": "supabase stop",
    "email:dev": "email dev --dir ./emails/",
    "email:build": "email dev --dir ./src/emails"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@react-email/components": "0.0.15",
    "@react-email/render": "^0.0.12",
    "@supabase/auth-helpers-nextjs": "^0.9.0",
    "@supabase/ssr": "^0.0.10",
    "@supabase/supabase-js": "^2.39.3",
    "date-fns": "^3.3.1",
    "lucide-react": "^0.323.0",
    "next": "14.1.0",
    "postmark": "^4.0.2",
    "react": "^18",
    "react-dom": "^18",
    "react-email": "2.0.0",
    "tailwind-merge": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@snaplet/copycat": "^2.0.0",
    "@snaplet/seed": "^0.84.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "postcss": "^8",
    "snaplet": "^0.84.0",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
